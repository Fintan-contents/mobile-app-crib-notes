{
  "name": "santoku-app",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android --appId jp.fintan.mobile.SantokuApp --variant devSantokuAppDebug --appIdSuffix dev.debug",
    "android:debugAdvanced": "react-native run-android --appId jp.fintan.mobile.SantokuApp --variant devSantokuAppDebugAdvanced --appIdSuffix dev.debugAdvanced",
    "ios": "react-native run-ios --scheme DevSantokuApp --configuration Debug",
    "ios:debugAdvanced": "react-native run-ios --scheme DevSantokuApp --configuration DebugAdvanced",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "jest --detectOpenHandles --runInBand",
    "test:coverage": "jest --detectOpenHandles --runInBand --coverage",
    "test:report": "jest --detectOpenHandles --runInBand --reporters=default --reporters=jest-junit --coverage",
    "lint": "run-s --print-name --continue-on-error lint:*",
    "lint:es": "eslint --ext .jsx,.js,.tsx,.ts .",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-s --print-name --continue-on-error fix:*",
    "fix:es": "eslint --fix --ext .jsx,.js,.tsx,.ts .",
    "reset-cache": "node .script/runner.js reset-cache",
    "reset-cache:all": "node .script/runner.js reset-cache --all",
    "reset-cache:interactive": "node .script/runner.js reset-cache --interactive",
    "pod-install": "cd ios && bundle exec pod install --repo-update",
    "postinstall": "run-s postinstall:*",
    "postinstall:patch-package": "patch-package",
    "bundle": "run-s --print-name --continue-on-error bundle:*",
    "bundle:android": "mkdirp ./generated && react-native bundle --dev false --sourcemap-output ./generated/index.android.bundle.map --entry-file index.js --bundle-output ./generated/index.android.bundle --platform android",
    "bundle:ios": "mkdirp ./generated && react-native bundle --dev false --sourcemap-output ./generated/index.ios.bundle.map --entry-file index.js --bundle-output ./generated/index.ios.bundle --platform ios",
    "symbolicate:android": "npx metro-symbolicate ./generated/index.android.bundle.map",
    "symbolicate:ios": "npx metro-symbolicate ./generated/index.ios.bundle.map",
    "orval": "npx orval --config ./orval.config.ts"
  },
  "dependencies": {
    "@mswjs/data": "^0.10.1",
    "@notifee/react-native": "~5.6.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.1.2",
    "@react-native-community/netinfo": "8.2.0",
    "@react-native-firebase/app": "~14.11.0",
    "@react-native-firebase/crashlytics": "~14.11.0",
    "@react-native-firebase/messaging": "~14.11.0",
    "@react-native-masked-view/masked-view": "0.2.6",
    "@react-native-picker/picker": "2.4.0",
    "@react-navigation/bottom-tabs": "~6.3.0",
    "@react-navigation/core": "~6.3.0",
    "@react-navigation/drawer": "~6.4.0",
    "@react-navigation/elements": "~1.3.0",
    "@react-navigation/native": "~6.0.6",
    "@react-navigation/native-stack": "~6.8.0",
    "@react-navigation/stack": "~6.2.0",
    "@shopify/restyle": "^2.1.0",
    "axios": "^0.27.0",
    "expo": "^45.0.0",
    "expo-crypto": "~10.2.0",
    "expo-keep-awake": "~10.1.1",
    "expo-local-authentication": "~12.2.0",
    "expo-random": "~12.2.0",
    "expo-secure-store": "~11.2.0",
    "expo-splash-screen": "~0.15.1",
    "formik": "~2.2.9",
    "msw": "^0.44.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-config": "~1.4.0",
    "react-native-elements": "~3.4.0",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "0.17.7",
    "react-native-webview": "11.18.1",
    "react-query": "^3.34.8",
    "yup": "~0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/react-native": "^11.0.0",
    "@types/jest": "<27.0.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@types/react-test-renderer": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-preset-expo": "~9.1.0",
    "eslint": "^8.0.0",
    "eslint-config-universe": "^11.0.0",
    "eslint-formatter-rdjson": "^1.0.3",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-strict-dependencies": "^1.0.1",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.0",
    "jest-junit": "^14.0.0",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "orval": "^6.6.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.0",
    "react-test-renderer": "17.0.2",
    "typescript": "~4.3.5"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "outputName": "jest-junit.xml"
  },
  "private": true
}
