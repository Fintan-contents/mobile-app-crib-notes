{
  "name": "santoku-app",
  "main": "index.js",
  "scripts": {
    "android": "cross-env ENVIRONMENT=local expo run:android",
    "android:dev": "cross-env ENVIRONMENT=dev expo run:android",
    "android:stg": "cross-env ENVIRONMENT=stg expo run:android",
    "android:prod": "cross-env ENVIRONMENT=prod expo run:android",
    "ios": "cross-env ENVIRONMENT=local expo run:ios",
    "ios:dev": "cross-env ENVIRONMENT=dev expo run:ios",
    "ios:stg": "cross-env ENVIRONMENT=stg expo run:ios",
    "ios:prod": "cross-env ENVIRONMENT=prod expo run:ios",
    "web": "expo start --web",
    "start": "cross-env ENVIRONMENT=local expo start",
    "start:dev": "cross-env ENVIRONMENT=dev expo start",
    "start:stg": "cross-env ENVIRONMENT=stg expo start",
    "start:prod": "cross-env ENVIRONMENT=prod expo start",
    "test": "jest --runInBand --forceExit",
    "prebuild": "run-s build:plugin && cross-env ENVIRONMENT=local expo prebuild --clean --no-install",
    "prebuild:dev": "run-s build:plugin && cross-env ENVIRONMENT=dev expo prebuild --clean --no-install",
    "prebuild:stg": "run-s build:plugin && cross-env ENVIRONMENT=stg expo prebuild --clean --no-install",
    "prebuild:prod": "run-s build:plugin && cross-env ENVIRONMENT=prod expo prebuild --clean --no-install",
    "postprebuild": "run-p copy:local:*",
    "postprebuild:dev": "run-p copy:dev:*",
    "postprebuild:stg": "run-p copy:stg:*",
    "postprebuild:prod": "run-p copy:prod:*",
    "build:plugin": "rimraf config/plugin/build && tsc --build config/plugin",
    "copy:local:android": "rimraf prebuild/local/android && cpx \"android/**/{.*,*}\" prebuild/local/android",
    "copy:local:ios": "rimraf prebuild/local/ios && cpx \"ios/**/{.*,*}\" prebuild/local/ios",
    "copy:dev:android": "rimraf prebuild/dev/android && cpx \"android/**/{.*,*}\" prebuild/dev/android",
    "copy:dev:ios": "rimraf prebuild/dev/ios && cpx \"ios/**/{.*,*}\" prebuild/dev/ios",
    "copy:stg:android": "rimraf prebuild/stg/android && cpx \"android/**/{.*,*}\" prebuild/stg/android",
    "copy:stg:ios": "rimraf prebuild/stg/ios && cpx \"ios/**/{.*,*}\" prebuild/stg/ios",
    "copy:prod:android": "rimraf prebuild/prod/android && cpx \"android/**/{.*,*}\" prebuild/prod/android",
    "copy:prod:ios": "rimraf prebuild/prod/ios && cpx \"ios/**/{.*,*}\" prebuild/prod/ios",
    "test:coverage": "jest --runInBand --forceExit --coverage",
    "test:report": "jest --runInBand --forceExit --reporters=default --reporters=jest-junit --coverage",
    "lint": "run-s --print-name --continue-on-error lint:*",
    "lint:es": "eslint --ext .jsx,.js,.tsx,.ts .",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-s --print-name --continue-on-error fix:*",
    "fix:es": "eslint --fix --ext .jsx,.js,.tsx,.ts .",
    "reset-cache": "node .script/runner.js reset-cache",
    "reset-cache:all": "node .script/runner.js reset-cache --all",
    "reset-cache:interactive": "node .script/runner.js reset-cache --interactive",
    "pod-install": "cd ios && bundle exec pod install --repo-update",
    "postinstall": "run-s postinstall:*",
    "postinstall:patch-package": "patch-package",
    "bundle": "run-s --print-name --continue-on-error bundle:*",
    "bundle:android": "mkdirp ./generated && react-native bundle --dev false --sourcemap-output ./generated/index.android.bundle.map --entry-file index.js --bundle-output ./generated/index.android.bundle --platform android",
    "bundle:ios": "mkdirp ./generated && react-native bundle --dev false --sourcemap-output ./generated/index.ios.bundle.map --entry-file index.js --bundle-output ./generated/index.ios.bundle --platform ios",
    "symbolicate:android": "npx metro-symbolicate ./generated/index.android.bundle.map",
    "symbolicate:ios": "npx metro-symbolicate ./generated/index.ios.bundle.map",
    "orval": "npx orval --config ./orval.config.ts"
  },
  "dependencies": {
    "@mswjs/data": "^0.14.0",
    "@notifee/react-native": "~5.7.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-firebase/app": "~18.5.0",
    "@react-native-firebase/crashlytics": "~18.5.0",
    "@react-native-firebase/messaging": "~18.5.0",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "~6.6.0",
    "@react-navigation/core": "~6.4.0",
    "@react-navigation/drawer": "~6.7.0",
    "@react-navigation/elements": "~1.3.0",
    "@react-navigation/native": "~6.1.0",
    "@react-navigation/native-stack": "~6.11.0",
    "@react-navigation/stack": "~6.4.0",
    "@shopify/restyle": "^2.1.0",
    "@tanstack/react-query": "~4.36.0",
    "axios": "~1.6.0",
    "expo": "^51.0.0",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.5",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.26",
    "expo-keep-awake": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "formik": "~2.4.0",
    "jsbarcode": "~3.11.5",
    "msw": "^1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-elements": "~3.4.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-qrcode-svg": "~6.3.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.8.6",
    "yup": "~1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@expo/config-plugins": "~8.0.0",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react-native": "^10.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "<30.0.0",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~11.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-formatter-rdjson": "^1.0.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-strict-dependencies": "^1.0.1",
    "expo-module-scripts": "^3.0.4",
    "fs-extra": "^11.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "jest-junit": "^16.0.0",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "orval": "^6.6.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "outputName": "jest-junit.xml"
  },
  "private": true,
  "version": "1.0.0"
}
